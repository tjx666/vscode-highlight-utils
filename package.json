{
    "name": "highlight-utils",
    "displayName": "Highlight Utils",
    "publisher": "zardoy",
    "version": "0.0.0-dev",
    "license": "MIT",
    "categories": [
        "Other"
    ],
    "contributes": {
        "commands": [
            {
                "command": "goToHighlightedLocations",
                "title": "Go to Highlighted Locations"
            },
            {
                "command": "goToHighlightedLocationsRead",
                "title": "Go to Highlighted Locations (Read)"
            },
            {
                "command": "goToHighlightedLocationsWrite",
                "title": "Go to Highlighted Locations (Write)"
            },
            {
                "command": "replaceHighlights",
                "title": "Replace Highlights With..."
            }
        ]
    },
    "activationEvents": [
        "onCommands"
    ],
    "keywords": [
        "highlights",
        "peek",
        "references",
        "same file locations"
    ],
    "scripts": {
        "start": "vscode-framework start",
        "build": "tsc && vscode-framework build",
        "lint": "eslint src/**"
    },
    "dependencies": {
        "@zardoy/vscode-utils": "^0.0.46",
        "string-dedent": "^3.0.1",
        "vscode-framework": "^0.0.18"
    },
    "devDependencies": {
        "@types/vscode": "1.70.0",
        "@zardoy/tsconfig": "^1.5.0",
        "eslint": "^8.28.0",
        "eslint-config-zardoy": "^0.2.15",
        "typescript": "^4.9.3"
    },
    "prettier": {
        "semi": false,
        "singleQuote": true,
        "proseWrap": "never",
        "tabWidth": 4,
        "trailingComma": "all",
        "arrowParens": "avoid",
        "printWidth": 160,
        "endOfLine": "auto"
    }
}
